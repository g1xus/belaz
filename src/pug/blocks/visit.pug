mixin visit
    section.visit.section.dark
        .container
            .section-headline
                h3.section-headline__title
                    | Приезжайте в наш офис!
            .visit-wrapper
                address.visit__address
                    | г. Архангельск, Москва-Сити, оф. 3725
                a(href="tel: +7 (818) 247-47-25").visit__link.dark-link
                    | +7 (818) 247-47-25
